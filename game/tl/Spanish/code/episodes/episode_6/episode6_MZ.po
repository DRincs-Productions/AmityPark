# game/code/episodes/episode_6/episode6_MZ.rpy:1360 #-#-# translate crowdin The_Great_Out_Doors_scene_8a_2_4e7503d1:
msgid "[_danny_s_phantom_s_towards_s_wondering_s_talk_frown_] My name's Danny... Danny Fenton."
msgstr "[_danny_s_phantom_s_towards_s_wondering_s_talk_frown_] Mi nombre es Danny... Danny Fenton."

# game/code/episodes/episode_6/episode6_MZ.rpy:1363 #-#-# translate crowdin The_Great_Out_Doors_scene_8a_2_3592ce3e:
msgid "[_delvo_s_towards_s_sad_s_talk_frown_] I... I am Sir Delvo. I apologize for my rashness, young Danny."
msgstr "[_delvo_s_towards_s_sad_s_talk_frown_] yo... soy Sir Delvo. Me disculpo por mi actitud, joven Danny."

# game/code/episodes/episode_6/episode6_MZ.rpy:1366 #-#-# translate crowdin The_Great_Out_Doors_scene_8a_2_0808babe:
