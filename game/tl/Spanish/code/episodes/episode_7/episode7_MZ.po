# game/code/episodes/episode_7/episode7_MZ.rpy:8563 #-#-# translate crowdin A_Nightmare_Called_Ember_desiree_1_continued_756fb5bb:
msgid "[_desiree_] ...and my help you shall receive, my Master..."
