    danny sad talk_happy "Gracias, Des."

# game/code/misc/Desiree_Dialogue.rpy:1917
translate spanish call_me_blank_4b94a1fb:

    # desiree closed talk_happy "Of course, master [player_name]. What else do you require of me?"
    desiree closed talk_happy "Por supuesto, maestro [player_name]. ¿Qué más requiere de mí?"

